    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@ai-sdk/google": "^1.1.18",
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/to-ico": "^1.1.3",
    "ai": "^4.1.50",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
